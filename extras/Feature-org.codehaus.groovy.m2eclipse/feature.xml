<?xml version="1.0" encoding="UTF-8"?>

<feature
      label="%featureName"
      version="3.0.0.qualifier"
      provider-name="%providerName"
      id="org.codehaus.groovy.m2eclipse.feature">

   <description url="%descriptionURL">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.codehaus.groovy" version="2.1.9" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.13.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.groovy.core" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.codehaus.groovy.eclipse.core" version="3.0.0" match="greaterOrEqual"/>
      <import feature="org.codehaus.groovy.eclipse.feature" version="3.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.codehaus.groovy.m2eclipse"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"
   />

</feature>
